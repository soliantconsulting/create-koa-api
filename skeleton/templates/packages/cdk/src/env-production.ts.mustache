import { App } from "aws-cdk-lib";
import { ApiStack } from "./api-stack.js";

const app = new App();

throw new Error("Production env not configured");

// biome-ignore lint/correctness/noUnreachable: env not configured
new ApiStack(app, "{{{apiName}}}-production", {
    env: {
        account: "{{{accountId}}}",
        region: "{{{region}}}",
    },
    apiEnv: "production",
    certificateArn: "",
{{#if (has features "zoom_error_log_notifications")}}
    zoomWebhookSecretArn: "{{{zoomWebhook.productionSecretArn}}}",
{{/if}}
});

app.synth();
