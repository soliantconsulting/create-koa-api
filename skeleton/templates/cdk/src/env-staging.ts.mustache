import { App } from "aws-cdk-lib";
import "source-map-support/register.js";
import { ApiStack } from "./api-stack.js";

const app = new App();

new ApiStack(app, "{{{apiName}}}-staging", {
    env: {
        account: "{{{accountId}}}",
        region: "{{{region}}}",
    },
    apiEnv: "staging",
    certificateArn: "{{{stagingCertificateArn}}}",
});

app.synth();
